<script type='text/javascript'>
    var adSlot = '/19196947/dilbert.com/home';


    var anchor;
    var tower_permalink;
    var tower_permalink_refresh = 0;
    var tower_permalink_interval;
    var adSizes = {};
    var adSizeMappings = {};
    var adTargets = {};
    var slotName;
    var pageWidth = $(window).width();
    var pageHeight = $(window).height();
    var interstitial = false;


    // Interstitial Cookie Logic
    function specialAds() {
        if($('body.strips').length && pageWidth <= 588) {
            //console.log(pageWidth);
            var $pc = parseInt($.cookie('page_count'), 10);
            $pc >= 0 ? $pc++ : $pc = 0;
            $.cookie('page_count', $pc, {expires: 1, path: '/strip'});
            //console.log('The page count is: ' + $pc);
            if($pc in objectConverter([3, 8, 13, 18, 23, 28, 33])) {
                //console.log('interstitial is true');
                // $('.ad-interstitial-wrapper').removeClass('hide').find('.ad-interstitial').addClass('ad');
                $('.ad-interstitial').addClass('js-ad');
                interstitial = true;
            }
        } else {
            $pc = 0;
            $.cookie('page_count', 0, { expires: 1, path: '/strip' });
        }
    }
    

    /* ****************** */
    /*      Ad Sizes      */
    /* ****************** */
    adSizes['leaderboard'] = [[970, 250], [970, 90], [728, 90], [320, 50]];
    adSizes['banner'] = [[970, 250], [970, 90], [728, 90], [320, 50]];
    adSizes['rectangle'] = [[300, 250]];
    adSizes['rectangle_right'] = [[300, 250]];
    adSizes['tower'] = [[300, 600], [160, 600], [300, 250]];
    adSizes['tower_permalink'] = [[300, 600], [160, 600]];
    adSizes['anchored_tower'] = [[160, 600]];
    adSizes['interstitial'] = [[1,1]];


    /* ********************** */
    /*      Ad Targeting      */
    /* ********************** */
    adTargets['leaderboard'] = 'top';
    adTargets['banner'] = 'bot';
    adTargets['rectangle'] = 'left';
    adTargets['rectangle_right'] = 'right';
    adTargets['tower'] = 'content';
    adTargets['tower_permalink'] = 'comments';
    adTargets['anchored_tower'] = 'anchored';
    adTargets['interstitial'] = 'interstitial';

    googletag.cmd.push(function() {
        /* ************************** */
        /*      Ad Size Mappings      */
        /* ************************** */
        adSizeMappings['leaderboardMapping'] = googletag.sizeMapping().
                addSize([970, 851], [[970, 250], [970, 90], [728, 90]]).
                addSize([970, 0], [[970, 90], [728, 90]]).
                addSize([728, 0], [[728, 90], [320, 50]]).
                addSize([0, 0], [320, 50]).
                build();

        adSizeMappings['bannerMapping'] = googletag.sizeMapping().
                addSize([970, 0], [[970, 250], [970, 90], [728, 90]]).
                addSize([728, 0], [[728, 90], [320, 50]]).
                addSize([0, 0], [320, 50]).
                build();

        adSizeMappings['rectangleMapping'] = googletag.sizeMapping().
                addSize([0, 0], [[300, 250]]).
                build();

        adSizeMappings['rectangle_rightMapping'] = googletag.sizeMapping().
                addSize([992, 0], [[300, 250]]).
                addSize([0, 0], []).
                build();

        adSizeMappings['towerMapping'] = googletag.sizeMapping().
                addSize([992, 0], [[300, 600], [160, 600]]).
                addSize([551, 0], [160, 600]).
                addSize([0, 0], [300, 250]).
                build();

        adSizeMappings['tower_permalinkMapping'] = googletag.sizeMapping().
                addSize([992, 0], [[300, 600], [160, 600]]).
                addSize([768, 0], [160, 600]).
                addSize([0, 0], []).
                build();

        adSizeMappings['anchored_towerMapping'] = googletag.sizeMapping().
                addSize([1400, 620], [[160, 600]]).
                addSize([0, 0], []).
                build();

        adSizeMappings['interstitialMapping'] = googletag.sizeMapping().
                addSize([0, 0], [[1, 1]]).
                build();



        /* *************************** */
        /*      Initial Page Load      */
        /* *************************** */
        $(document).ready(function(){
            var curAds = findAds("html");
            var curSlots = defineAds(curAds);

            googletag.enableServices();

            displayAds(curAds);

            tower_permalink_interval = setInterval(function () { refresh_tower_permalink(); }, 120000);

            googletag.pubads().addEventListener('slotRenderEnded', function(event) {
                if (event.slot === tower_permalink) {
                    if (event.isEmpty) {
                        clearInterval(tower_permalink_interval);
                    }
                }
            });
        });
    });




    /* ********************************** */
    /*      Helper Functions for Ads      */
    /* ********************************** */

    function refresh_tower_permalink(){
        if(tower_permalink_refresh < 2){
            googletag.pubads().refresh([tower_permalink]);
            tower_permalink_refresh++;
        } else {
            clearInterval(tower_permalink_interval);
        }
    }


    function findAds(startPoint) {
        // Starting point
        var sp = document.querySelectorAll(startPoint);
        // Place to store ad slot divs
        var ads;

        // Find all ads after the starting point
        if(sp.length != 0) {
            ads = $(sp).find('.js-ad');
        } else {
            ads = document.querySelectorAll('.js-ad');
        }

        return ads;
    }


    function defineAds(adDivs) {
        var adDivsLength = adDivs.length;

        var adType, slotID, slot;

        // Place to store slot objects
        var adSlots = [];
        for (var i = adDivsLength-1; i>=0; i--) {
            // ID given to ad slot, usually ad type + index
            slotName = adDivs[i].id;

            adType = slotName.replace(/[0-9]/g, '');
            slotID = slotName.match(/\d+/g, '');

            slot = googletag.defineSlot(adSlot, adSizes[adType], slotName).defineSizeMapping(adSizeMappings[adType+'Mapping']).addService(googletag.pubads()).setTargeting('pos', adTargets[adType]);
            adSlots.push(slot);

            // If it is the anchored tower slot or tower_permalink spot, store it
            // for later reference
            if(slotName == 'anchored_tower') {
                anchor = slot;
            } else if (slotName == 'tower_permalink') {
                tower_permalink = slot;
            }

        }
        return adSlots;
    }


    function displayAds(adDivs) {
        var adDivsLength = adDivs.length;
        var slotName;

        if(adDivsLength == 0) {
            googletag.display();
        } else {
            for (var i = adDivsLength-1; i>=0; i--) {
                slotName = adDivs[i].id;
                googletag.display(slotName);
            }
        }

    }


    function refreshAds(adSlots) {
        if (adSlots.length == 0) {
            googletag.pubads().refresh().collapseEmptyDivs(true);
        } else {
            googletag.pubads().refresh(adSlots).collapseEmptyDivs(true);
        }
    }

</script>

